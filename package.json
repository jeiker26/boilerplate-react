{
  "name": "boilerplate-react",
  "version": "1.0.0",
  "description": "boilerplate for react",
  "main": "index.js",
  "scripts": {
    "lint": "eslint src",
    "test": "jest --watchAll --no-cache --config ./config/jest.config.json",
    "start": "webpack-dev-server --open --config ./config/webpack.dev.js",
    "build": "webpack --config ./config/webpack.prod.js",
    "storybook": "start-storybook",
    "cy:open": "cypress open",
    "cy:run": "cypress run -- --record"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jeiker26/boilerplate-react.git"
  },
  "keywords": [
    "react"
  ],
  "author": "jesus javega",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jeiker26/boilerplate-react/issues"
  },
  "homepage": "https://github.com/jeiker26/boilerplate-react#readme",
  "devDependencies": {
    "@babel/core": "^7.2.2",
    "@babel/plugin-proposal-class-properties": "^7.1.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.0.0",
    "@babel/preset-env": "^7.2.3",
    "@babel/preset-react": "^7.0.0",
    "@storybook/react": "^5.0.1",
    "autoprefixer": "^9.4.10",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^23.6.0",
    "babel-loader": "^8.0.5",
    "babel-plugin-module-resolver": "^3.2.0",
    "babel-polyfill": "^6.26.0",
    "clean-webpack-plugin": "^2.0.0",
    "css-loader": "^2.1.1",
    "cypress": "^3.1.5",
    "eslint": "^5.12.1",
    "eslint-config-prettier": "^4.0.0",
    "eslint-loader": "^2.1.2",
    "eslint-plugin-prettier": "^3.0.1",
    "eslint-plugin-react": "^7.12.4",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^1.3.1",
    "jest": "^23.6.0",
    "jsdom": "^13.2.0",
    "lint-staged": "^8.1.1",
    "mini-css-extract-plugin": "^0.5.0",
    "node-sass": "^4.11.0",
    "postcss-loader": "^3.0.0",
    "prettier": "1.16.1",
    "react-hot-loader": "^4.6.5",
    "react-testing-library": "^6.0.0",
    "sass": "*",
    "sass-loader": "^7.1.0",
    "sinon": "^7.2.3",
    "style-loader": "^0.23.1",
    "uglifyjs-webpack-plugin": "^2.1.2",
    "webpack": "^4.29.0",
    "webpack-cli": "^3.2.1",
    "webpack-dev-server": "^3.1.14",
    "webpack-merge": "^4.2.1"
  },
  "lint-staged": {
    "*.js": [
      "npm run lint",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "dependencies": {
    "i18next": "^15.0.6",
    "i18next-browser-languagedetector": "^3.0.1",
    "i18next-xhr-backend": "^2.0.1",
    "react": "^16.8.4",
    "react-dom": "^16.8.4",
    "react-i18next": "^10.3.1",
    "react-router": "^4.3.1",
    "react-router-dom": "^4.3.1"
  }
