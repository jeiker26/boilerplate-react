{
    "presets": [
      "@babel/env",
      "@babel/react"
    ],
    "plugins": [
      ["@babel/proposal-class-properties"],
      ["@babel/proposal-object-rest-spread"],
      [
        "module-resolver",
        {
          "root": ["."],
          "alias": {
            "src": "./src"
          }
        }
      ]

    ]
  }
  